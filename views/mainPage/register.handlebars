    <div class="container">
        <form id="form" action="/user/login">
            <div class="input-control">
                <label for="username">Username</label>
                <input id="username" name="username" type="text">
                <div class="error"></div>
            </div>
            <div class="input-control">
                <label for="email">Email</label>
                <input id="email" name="email" type="text">
                <div class="error"></div>
            </div>
            
            <div class="input-control">
          <label id="number-label">Age</label>
          <input type="number" id="age" name="age" placeholder="Age" min=16 max=120><br>
                <div class="error"></div>
            </div>
            <div class="input-control">
                <label for="city">City</label>
                {{!-- <input id="city"name="city" type="text"> --}}
                <select id="city" name="city">
              <option value="bayonne">Bayonne</option>
            <option value="harrison">Harrison</option>
              <option value="hoboken">Hoboken</option>
            <option value="jerseyc">Jersey City</option>
            <option value="kearny">Kearny</option>
            <option value="northb">North Bergen</option>
            <option value="secaucus">Secaucus</option>
            <option value="unionc">Union City</option>
            <option value="weehawken">Weehawken</option>
            <option value="westny">West New York</option>
            </select>
                <div class="error"></div>
            </div>
            <div class="input-control">
                <label for="password">Password</label>
                <input id="password"name="password" type="password">
                <div class="error"></div>
            </div>
            <div class="input-control">
                <label for="password2">Password again</label>
                <input id="password2"name="password2" type="password">
                <div class="error"></div>
            </div>
            <button type="submit">Sign Up</button>
        </form>
    </div>
